{
  "manifest_version": 2,
  "name": "Yodafy",
  "version": "1.0",
  "description": "Translate incomprehensible English into simple-to-understand Yoda grammar.",
  "content_scripts":[
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["./main.js", "./grabText.js", "./jquery-3.4.1.min.js"]
    }
  ],
  "icons": { "16": "./assets/Yoda16.png",
    "48": "./assets/Yoda48.png",
   "128": "./assets/Yoda128.png" }
}
